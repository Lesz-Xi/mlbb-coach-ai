  "role_definitions": {
    "tank": {
      "description": "Frontline initiators and damage soakers",
      "key_metrics": [
        "damage_taken",
        "crowd_control",
        "teamfight_participation"
      ],
      "evaluation_focus": "initiation_success, damage_absorption, peel_effectiveness"
    },
    "fighter": {
      "description": "Versatile bruisers with sustain and damage",
      "key_metrics": ["damage_dealt", "damage_taken", "sustain"],
      "evaluation_focus": "damage_trade_efficiency, sustain_effectiveness, objective_control"
    },
    "assassin": {
      "description": "High mobility burst damage dealers",
      "key_metrics": ["damage_dealt", "elimination_efficiency", "mobility"],
      "evaluation_focus": "target_elimination, survival_rate, snowball_potential"
    },
    "marksman": {
      "description": "Ranged physical damage carries",
      "key_metrics": ["damage_dealt", "positioning", "gold_efficiency"],
      "evaluation_focus": "damage_consistency, positioning_safety, item_optimization"
    },
    "mage": {
      "description": "Magical damage dealers and controllers",
      "key_metrics": ["damage_dealt", "ability_accuracy", "mana_efficiency"],
      "evaluation_focus": "burst_effectiveness, skill_accuracy, team_control"
    },
    "support": {
      "description": "Utility providers and team enablers",
      "key_metrics": ["assists", "utility_provided", "vision_control"],
      "evaluation_focus": "team_enablement, utility_usage, map_control"
    }
  },
  "sub_role_definitions": {
    "burst": { "description": "High burst damage in short windows" },
    "sustain": { "description": "Sustained damage and survivability" },
    "mobility": { "description": "High mobility and positioning" },
    "control": { "description": "Crowd control and area denial" },
    "carry": { "description": "Late-game damage scaling" },
    "initiator": { "description": "Team fight initiation" },
    "stealth": { "description": "Stealth mechanics and surprise attacks" },
    "healer": { "description": "Team healing and sustain" },
    "utility": { "description": "Utility and team support" },
    "poke": { "description": "Long-range harassment" },
    "anti-mobility": { "description": "Counters mobile champions" },
    "roamer": { "description": "Map mobility and roaming" },
    "bruiser": { "description": "Tanky damage dealer" },
    "assassin": { "description": "High single-target damage" },
    "hybrid": { "description": "Multiple role capabilities" }
  }
}
